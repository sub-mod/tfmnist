{
	"apiVersion": "tensorflow.org/v1alpha1",
	"kind": "TfJob",
	"metadata": {
		"name": "tfjob-gpu"
	},
	"spec": {
		"replicaSpecs": [
			{
				"template": {
					"spec": {
						"containers": [
							{
								"image": "submod/gputest1:latest",
								"name": "tensorflow",
								"resources": {
									"requests": {
										"alpha.kubernetes.io/nvidia-gpu": 1
									}
								}
							}
						],
						"restartPolicy": "OnFailure"
					}
				}
			}
		]
	},
        "tfImage": "submod/gputest1:latest"
}
